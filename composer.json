{
    "name": "sgtw/sgtw",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "aws/aws-sdk-php": "^3.166",
        "fideloper/proxy": "~3.3",
        "graham-campbell/flysystem": "^5.1",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "ixudra/curl": "^6.16",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "league/flysystem-sftp": "^1.0",
        "neoxia/laravel-sftp": "^1.0",
        "shivella/laravel-bitly": "^1.1",
        "yab/laracogs": "^2.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "files" : [
            "app/Helpers/activity_helper.php",
            "app/Services/ECPay.php",
            "app/Services/EsafePay.php",
            "app/Library/class.fileuploader.php"
        ],

        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
