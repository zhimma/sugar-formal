{
    "name": "sgtw/sgtw",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3|^8.0",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.166",
        "beyondcode/laravel-websockets": "^1.12",
        "facade/ignition": "^2.3.6",
        "fideloper/proxy": "^4.0",
        "graham-campbell/flysystem": "^7",
        "guzzlehttp/guzzle": "^7.0.1",
        "inspector-apm/inspector-laravel": "^4.7",
        "intervention/image": "^2.4",
        "ixudra/curl": "^6.16",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.4",
        "laravel/tinker": ">=1.0",
        "laravel/ui": "^3.4",
        "league/flysystem-sftp": "^1.0",
        "livewire/livewire": "^2.8",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "pusher/pusher-php-server": "~3.0",
        "sentry/sentry-laravel": "^2.10",
        "shivella/laravel-bitly": "^1.1",
        "spatie/ray": "^1.34",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.1",
        "dietercoopman/laravel-showsql": "^1.1",
        "filp/whoops": "~2.0",
        "mockery/mockery": ">=0.9"
    },
    "autoload": {
        "files" : [
            "app/Helpers/activity_helper.php",
            "app/Services/ECPay.php",
            "app/Services/EsafePay.php",
            "app/Library/class.fileuploader.php",
            "app/Helpers/helpers.php",
            "app/Helpers/CloudFlareHelper.php"
        ],

        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi",
            "@php artisan clear-compiled",
            "@php artisan optimize",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
